{
   "$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Contact Schema",
	"description": "A single contact record from the solve360 API",
	"type": "object",
	"properties": {
		"created": { "$ref": "date-string.schema.json" },
		"flagged": {
			"type": "boolean"
		},
		"id": {
			"type": "integer"
		},
		"name": {
			"description": "contacts first and last name",
			"type": "string"
		},
		"parentcn": {
			"type": "string"
		},
		"parentid": {
			"type": "integer"	
		},
		"typeid": {
			"type": "integer"
		},
		"updated": { "$ref": "date-string.schema.json" },
		"viewed": { "$ref": "date-string.schema.json" }
	},
	"required": []
}
